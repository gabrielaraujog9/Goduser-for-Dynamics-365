{
  "manifest_version": 3,
  "name": "Goduser For Dynamics 365",
  "description": "Add the reading time to Chrome Extension documentation articles",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_title": "Goduser For Dynamics 365"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "/scripts/app.js"
      ]
    }
  ],
  "host_permissions": [
    "*://*.dynamics.com/api/*",
    "*://*.dynamics.com.mcas.ms/*"
  ]
}